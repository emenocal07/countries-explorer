{
  "expo": {
    "name": "Countries explorer",
  "slug": "countries-explorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/splash-icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "."
    },
    "plugins": [
      "expo-localization",
      "expo-router",
      "expo-font"
    ],
    "scheme": "explorer-app"
  }
}
